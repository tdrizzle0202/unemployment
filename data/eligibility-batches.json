{
  "metadata": {
    "description": "State UI eligibility batch logic configuration",
    "version": "2025-01",
    "source_file": "state-ui-rules-2025.json"
  },
  "modules": {
    "HQ_MULTIPLIER": {
      "id": "hq_multiplier",
      "name": "High Quarter Multiplier",
      "description": "Total base period wages must be >= multiplier × highest quarter wages",
      "formula": "BPW >= multiplier × HQW",
      "required_inputs": ["base_period_wages", "quarterly_wages"],
      "params": ["multiplier"]
    },
    "WBA_MULTIPLIER": {
      "id": "wba_multiplier",
      "name": "WBA Multiple",
      "description": "Total base period wages must be >= multiplier × weekly benefit amount",
      "formula": "BPW >= multiplier × WBA",
      "required_inputs": ["base_period_wages", "weekly_benefit_amount"],
      "params": ["multiplier"],
      "note": "WBA must be calculated first before eligibility can be determined"
    },
    "FLAT_FLOOR": {
      "id": "flat_floor",
      "name": "Flat Dollar Floor",
      "description": "Minimum dollar amount required in base period",
      "formula": "BPW >= min_amount",
      "required_inputs": ["base_period_wages"],
      "params": ["min_amount"]
    },
    "HQ_FLOOR": {
      "id": "hq_floor",
      "name": "High Quarter Dollar Floor",
      "description": "Minimum dollar amount required in highest quarter",
      "formula": "HQW >= min_amount",
      "required_inputs": ["quarterly_wages"],
      "params": ["min_amount"]
    },
    "MULTI_QUARTER": {
      "id": "multi_quarter",
      "name": "Multiple Quarters Required",
      "description": "Wages must be present in multiple quarters",
      "formula": "quarters_with_wages >= min_quarters",
      "required_inputs": ["quarterly_wages"],
      "params": ["min_quarters"]
    },
    "OUTSIDE_HQ": {
      "id": "outside_hq",
      "name": "Wages Outside High Quarter",
      "description": "Minimum wages required outside the highest quarter",
      "formula": "BPW - HQW >= min_amount OR (BPW - HQW) >= percentage × BPW",
      "required_inputs": ["base_period_wages", "quarterly_wages"],
      "params": ["min_amount", "min_percentage"]
    },
    "HOURS_WORKED": {
      "id": "hours_worked",
      "name": "Hours Worked",
      "description": "Minimum hours of employment required",
      "formula": "hours >= min_hours",
      "required_inputs": ["hours_worked"],
      "params": ["min_hours"]
    },
    "WEEKS_WORKED": {
      "id": "weeks_worked",
      "name": "Weeks Worked",
      "description": "Minimum weeks of employment required",
      "formula": "weeks >= min_weeks",
      "required_inputs": ["weeks_worked"],
      "params": ["min_weeks"]
    },
    "MIN_WAGE_MULTIPLIER": {
      "id": "min_wage_multiplier",
      "name": "Minimum Wage Multiplier",
      "description": "Wages tied to state/federal minimum wage",
      "formula": "wages >= multiplier × minimum_wage",
      "required_inputs": ["base_period_wages", "state_minimum_wage"],
      "params": ["multiplier", "wage_type"]
    },
    "STATE_AWW": {
      "id": "state_aww",
      "name": "State Average Weekly Wage",
      "description": "Wages tied to state average weekly/annual wage",
      "formula": "wages >= percentage × state_aww",
      "required_inputs": ["base_period_wages", "state_average_wage"],
      "params": ["percentage", "wage_period"]
    }
  },
